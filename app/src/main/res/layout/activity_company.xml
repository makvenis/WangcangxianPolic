    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.makvenis.dell.wangcangxianpolic.company.CompanyActivity">

    <include layout="@layout/public_toolbar_simple_add"></include>

    <com.baoyz.widget.PullRefreshLayout
        android:id="@+id/company_swipe"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <com.makvenis.dell.wangcangxianpolic.view.SimpleRecycleScollView
            android:id="@+id/company_recycle"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

        </com.makvenis.dell.wangcangxianpolic.view.SimpleRecycleScollView>


    </com.baoyz.widget.PullRefreshLayout>





</LinearLayout>
